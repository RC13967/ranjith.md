////////////////////////Fix the code to rotate an array by k times and return rotated array using IIFE function/////////////////////
var arr = [1, 2, 3, 6, 8, 6, 1, 9, 11];
var k = 13;
k = k%arr.length;
(function() {
 out = {};
 out = arr.slice(k , arr.length);
 var count = out.length;
 for (var i = 0; i <= k + 1; i++) {
 out[count] = arr[i];
 count += 1;
 }
 console.log(out);})()

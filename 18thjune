friends1[1]="Mrs."+friends1[1]
friends1[2]="Mr."+friends1[2]
friends1[3]="Mr."+friends1[3]
friends1[4]="Mrs."+friends1[4]
friends1[5]="Mr."+friends1[5]
console.log(friends1)
